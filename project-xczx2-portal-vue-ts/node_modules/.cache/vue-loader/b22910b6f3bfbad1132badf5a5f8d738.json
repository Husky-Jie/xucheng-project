-block;\r\n}\r\n\r\n.register .signItem .phoneBox .textInput,\r\n.register .regItem .phoneBox .textInput {\r\n  width: 72%;\r\n}\r\n\r\n.register .signItem .phoneBox .codeSub,\r\n.register .regItem .phoneBox .codeSub {\r\n  height: 30px;\r\n  width: 94px;\r\n}\r\n\r\n.register .regItem {\r\n  display: none;\r\n  width: 350px;\r\n  position: relative;\r\n  margin: 0 auto;\r\n  text-align: left;\r\n  color: #787d82;\r\n}\r\n\r\n.register .regItem div {\r\n  position: relative;\r\n  margin: 15px 0;\r\n}\r\n\r\n.register .regItem div .proof {\r\n  display: none;\r\n  position: absolute;\r\n  bottom: -25px;\r\n  font-size: 12px;\r\n}\r\n\r\n.register .submitBut,\r\n.register .registerBut {\r\n  cursor: pointer;\r\n  font-size: 18px;\r\n  color: #f3f5f7;\r\n  background: #00a4ff;\r\n  text-align: center;\r\n  line-height: 40px;\r\n}\r\n\r\n.register footer {\r\n  color: #787d82;\r\n  background: #f3f5f7;\r\n  text-align: center;\r\n  bottom: 0px;\r\n}\r\n\r\n.register footer a {\r\n  display: inline-block;\r\n  padding: 0 40px;\r\n  margin-bottom: 20px;\r\n  line-height: 15px;\r\n  border-right: solid 1px #787d82;\r\n}\r\n</style>\r\n\r\n<style lang=\"scss\">\r\n.register {\r\n  .el-form-item__label {\r\n    padding: 0px;\r\n  }\r\n\r\n  .signItem .el-form-item__content {\r\n    .el-input {\r\n      margin: 0px;\r\n    }\r\n  }\r\n}\r\n</style>\r\n"]}]}